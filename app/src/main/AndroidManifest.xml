<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cudpast.appminas">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_app_mina3"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_app_mina3"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Principal.AllActivity" />


        <activity
            android:name=".Principal.AddPersonalActivity"
            android:parentActivityName=".Principal.AllActivity" />
        <activity
            android:name=".Principal.InputDataPersonalActivity"
            android:parentActivityName=".Principal.AllActivity" />
        <activity
            android:name=".Principal.ConsultaPersonalActivity"
            android:parentActivityName=".Principal.AllActivity" />

        <activity
            android:name=".Principal.DeletePersonalActivity"
            android:parentActivityName=".Principal.AllActivity" />


        <activity android:name=".Activities.EmptyActivity" />
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Principal.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>